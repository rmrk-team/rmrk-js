#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yuri/Sites/polkadot/nft-renderer/node_modules/.pnpm/react-native@0.73.2_@babel+core@7.23.7_@babel+preset-env@7.23.8_react@18.2.0/node_modules/react-native/node_modules:/Users/yuri/Sites/polkadot/nft-renderer/node_modules/.pnpm/react-native@0.73.2_@babel+core@7.23.7_@babel+preset-env@7.23.8_react@18.2.0/node_modules:/Users/yuri/Sites/polkadot/nft-renderer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yuri/Sites/polkadot/nft-renderer/node_modules/.pnpm/react-native@0.73.2_@babel+core@7.23.7_@babel+preset-env@7.23.8_react@18.2.0/node_modules/react-native/node_modules:/Users/yuri/Sites/polkadot/nft-renderer/node_modules/.pnpm/react-native@0.73.2_@babel+core@7.23.7_@babel+preset-env@7.23.8_react@18.2.0/node_modules:/Users/yuri/Sites/polkadot/nft-renderer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/react-native@0.73.2_@babel+core@7.23.7_@babel+preset-env@7.23.8_react@18.2.0/node_modules/react-native/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/react-native@0.73.2_@babel+core@7.23.7_@babel+preset-env@7.23.8_react@18.2.0/node_modules/react-native/cli.js" "$@"
fi
